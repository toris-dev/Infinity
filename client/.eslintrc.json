{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["prettier"],
  "plugins": ["prettier", "import"],
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": ["error"],
    "import/no-unresolved": "off",
    "import/extensions": "off",
    "import/no-cycle": "off",
    "import/no-extraneous-dependencies": "off",
    "class-methods-use-this": "off"
  }
}
